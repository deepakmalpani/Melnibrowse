{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Http/https test",
            "type": "debugpy",
            "request": "launch",
            "program": "browser.py",
            "console": "integratedTerminal",
            "args": "https://example.org/"
        },
        {
            "name": "Python Debugger: data scheme test",
            "type": "debugpy",
            "request": "launch",
            "program": "browser.py",
            "console": "integratedTerminal",
            "args": "'data:text/html,<h1>test</h1>'"
        },
        {
            "name": "Python Debugger: view-source scheme test",
            "type": "debugpy",
            "request": "launch",
            "program": "browser.py",
            "console": "integratedTerminal",
            "args": "view-source:https://example.org/"
        }
    ]
}